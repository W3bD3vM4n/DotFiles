// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+b",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+k ctrl+d",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "ctrl+k ctrl+f",
        "command": "editor.action.formatSelection",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "ctrl+x 2",
        "command": "workbench.action.splitEditorDown"
    },
    {
        "key": "ctrl+x 3",
        "command": "workbench.action.splitEditor"
    },
    {
        "key": "alt+j",
        "command": "editor.action.moveLinesDownAction"
    },
    {
        "key": "alt+k",
        "command": "editor.action.moveLinesUpAction"
    },
    {
        "key": "ctrl+shift+j",
        "command": "editor.unfold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+shift+k",
        "command": "editor.fold",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+alt+j",
        "command": "editor.unfoldAll",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+alt+k",
        "command": "editor.foldAll",
        "when": "editorTextFocus && foldingEnabled"
    },
    { "key": "ctrl+1", "command": "workbench.action.openEditorAtIndex1" },
    { "key": "ctrl+2", "command": "workbench.action.openEditorAtIndex2" },
    { "key": "ctrl+3", "command": "workbench.action.openEditorAtIndex3" },
    { "key": "ctrl+4", "command": "workbench.action.openEditorAtIndex4" },
    { "key": "ctrl+5", "command": "workbench.action.openEditorAtIndex5" },
    { "key": "ctrl+6", "command": "workbench.action.openEditorAtIndex6" },
    { "key": "ctrl+7", "command": "workbench.action.openEditorAtIndex7" },
    { "key": "ctrl+8", "command": "workbench.action.openEditorAtIndex8" },
    { "key": "ctrl+9", "command": "workbench.action.openEditorAtIndex9" },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "alt+enter",
        "command": "extension.liveServer.goOnline",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+backspace",
        "command": "extension.liveServer.goOffline",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+o",
        "command": "workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "alt+f",
        "command": "workbench.action.findInFiles"
    },
    {
        "key": "alt+g",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "alt+a",
        "command": "workbench.view.extension.sapling-sidebar-view"
    },
    {
        "key": "alt+d",
        "command": "workbench.view.debug",
        "when": "viewContainer.workbench.view.debug.enabled"
    },
    {
        "key": "alt+x",
        "command": "workbench.view.extensions",
        "when": "viewContainer.workbench.view.extensions.enabled"
    },
    {
        "key": "ctrl+n n",
        "command": "workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+n t",
        "command": "workbench.action.terminal.new",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+i",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+n f",
        "command": "welcome.showNewFileEntries"
    },
    {
        "key": "ctrl+alt+win+n",
        "command": "-welcome.showNewFileEntries"
    },
    {
        "key": "ctrl+n w",
        "command": "workbench.action.newWindow"
    },
    {
        "key": "ctrl+alt+l",
        "command": "workbench.action.moveEditorToRightGroup"
    },
    {
        "key": "ctrl+alt+h",
        "command": "workbench.action.moveEditorToLeftGroup"
    },
    {
        "key": "ctrl+alt+k",
        "command": "workbench.action.moveEditorToAboveGroup"
    },
    {
        "key": "ctrl+alt+j",
        "command": "workbench.action.moveEditorToBelowGroup"
    },
    {
        "key": "alt+1",
        "command": "workbench.action.focusFirstEditorGroup"
    },
    {
        "key": "alt+2",
        "command": "workbench.action.focusSecondEditorGroup"
    },
    {
        "key": "ctrl+p",
        "command": "-extension.vim_ctrl+p",
        "when": "editorTextFocus && vim.active && vim.use<C-p> && !inDebugRepl || vim.active && vim.use<C-p> && !inDebugRepl && vim.mode == 'CommandlineInProgress' || vim.active && vim.use<C-p> && !inDebugRepl && vim.mode == 'SearchInProgressMode'"
    },
    {
        "key": "ctrl+m",
        "command": "-extension.vim_ctrl+m",
        "when": "editorTextFocus && vim.active && vim.use<C-m> && !inDebugRepl || vim.active && vim.use<C-m> && !inDebugRepl && vim.mode == 'CommandlineInProgress' || vim.active && vim.use<C-m> && !inDebugRepl && vim.mode == 'SearchInProgressMode'"
    },
    {
        "key": "ctrl+n",
        "command": "-extension.vim_ctrl+n",
        "when": "editorTextFocus && vim.active && vim.use<C-n> && !inDebugRepl || vim.active && vim.use<C-n> && !inDebugRepl && vim.mode == 'CommandlineInProgress' || vim.active && vim.use<C-n> && !inDebugRepl && vim.mode == 'SearchInProgressMode'"
    },
    {
        "key": "win+v",
        "command": "-extension.vim_cmd+v",
        "when": "editorTextFocus && vim.active && vim.use<D-v> && !inDebugRepl && vim.mode == 'CommandlineInProgress' || editorTextFocus && vim.active && vim.use<D-v> && !inDebugRepl && vim.mode == 'SearchInProgressMode'"
    },
    {
        "key": "alt+b",
        "command": "workbench.view.extension.bito-activitybar"
    },
    {
        "key": "alt+i",
        "command": "workbench.view.extension.codium-sidebar-view-container"
    },
    {
        "key": "alt+t",
        "command": "workbench.view.extension.test"
    },
    {
        "key": "ctrl+shift+f4",
        "command": "workbench.action.closeAllEditors"
    },
    {
        "key": "ctrl+k ctrl+w",
        "command": "-workbench.action.closeAllEditors"
    },
    {
        "key": "alt+oem_comma",
        "command": "workbench.action.openSettings"
    },
    {
        "key": "ctrl+oem_comma",
        "command": "-workbench.action.openSettings"
    },
    {
        "key": "ctrl+oem_comma",
        "command": "extension.insertSemicolon",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+oem_2",
        "command": "-extension.insertSemicolon",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+oem_comma",
        "command": "extension.insertSemicolonWithNewLine",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+oem_2",
        "command": "-extension.insertSemicolonWithNewLine",
        "when": "editorTextFocus"
    }
]
